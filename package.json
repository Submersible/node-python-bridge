{
  "name": "python-bridge",
  "version": "1.1.0",
  "description": "Node.js to Python bridge ✨🐍🚀✨",
  "main": "index-compiled.js",
  "types": "index.d.ts",
  "scripts": {
    "lint": "npm run lint:ts",
    "lint:ts": "tslint --project tsconfig.json",
    "test": "npm run lint && npm run test:js && npm run test:ts",
    "test:js": "tap test-compiled.js",
    "test:ts": "tsc --lib ES2015 test_typescript.ts && tap test_typescript.js",
    "compile": "babel index.js --out-file index-compiled.js && babel test.js --out-file test-compiled.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Submersible/node-python-bridge.git"
  },
  "bugs": {
    "url": "https://github.com/Submersible/node-python-bridge/issues"
  },
  "homepage": "https://github.com/Submersible/node-python-bridge#readme",
  "keywords": [
    "python",
    "bridge",
    "ipc"
  ],
  "author": "Ryan Munro <ryan@submersible.io>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@types/node": "^8.0.14",
    "es6-promisify": "^6.0.1",
    "p-timeout": "^3.1.0",
    "tap": "^10.7.0",
    "temp": "^0.8.3",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": []
  }
}
